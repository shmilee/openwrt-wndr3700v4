
config global
	option anon_swap '0'
	option anon_mount '0'
	option auto_swap '1'
	option auto_mount '1'
	option delay_root '5'
	option check_fs '0'

config mount
	option enabled '1'
	#option uuid 'xxxxx-xxxx-xxxx'
	option label 'RouterUSB'
	#option device '/dev/sda1'
	option target '/mnt/sda1'
	option fstype 'ext4'

