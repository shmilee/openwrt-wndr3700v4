
config samba
	option name 'OpenWrt'
	option workgroup 'WORKGROUP'
	option description 'OpenWrt'
	option homes '0'

config sambashare
	option name 'RouterUSB'
	option path '/mnt/sda1'
	option read_only 'no'
	option users '{USER}'
	option guest_ok 'no'
	option create_mask '644'
	option dir_mask '755'

