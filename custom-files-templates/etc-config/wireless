config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/ar934x_wmac'
	option htmode 'HT20'
	option txpower '20'
	option country 'CN'
	option distance '10'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid '{SSID}'
	option encryption 'psk2'
	option key '{WIFIKEY}'

