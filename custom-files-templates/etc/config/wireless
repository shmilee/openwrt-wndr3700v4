config wifi-device 'radio0'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option band '2g'
	option channel '11'
	option cell_density '0'
	option country 'CN'
	option txpower '20'
	option htmode 'HT40'

config wifi-iface wifinet0
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid '{SSID}'
	option encryption 'sae-mixed'
	option key '{WIFIKEY}'
	option ocv '0'

